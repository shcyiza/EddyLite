<p id="notice"><%= notice %></p>

<h1>Sales</h1>

<table>
  <thead>
    <tr>
      <th>Saleable</th>
      <th>Transaction type</th>
      <th>Label share</th>
      <th>Artist share</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sales.each do |sale| %>
      <tr>
        <td><%= sale.saleable_id %></td>
        <td><%= sale.transaction_type %></td>
        <td><%= sale.label_share %></td>
        <td><%= sale.artist_share %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sale', new_sale_path %>
